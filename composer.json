{
    "name": "courselandia.ru/api",
    "type": "project",
    "description": "API courselandia.ru.",
    "keywords": ["API", "courselandia.ru"],
    "license": "MIT",
    "require": {
        "php": "^8.2.0",
        "ext-curl": "*",
        "ext-dom": "*",
        "ext-fileinfo": "*",
        "ext-ftp": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ext-mongodb": "*",
        "ext-pdo": "*",
        "ext-xmlreader": "*",
        "ext-zip": "*",
        "ckfinder/ckfinder-laravel-package": "^4.0",
        "codebuds/webp-converter": "^0.6.0",
        "danielme85/laravel-log-to-db": "^4.1.1",
        "firebase/php-jwt": "^6.10",
        "google/apiclient": "^2.14",
        "guzzlehttp/guzzle": "^7.5",
        "guzzlehttp/psr7": "^2.6",
        "intervention/image-laravel": "^1.2",
        "kalnoy/nestedset": "^6.0",
        "kix/mdash": "^0.5.5",
        "kreait/laravel-firebase": "^5.8.0",
        "laravel/framework": "^10.0",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.7",
        "menarasolutions/geographer-laravel": "^0.2.1",
        "meyfa/php-svg": "^0.15.0",
        "mongodb/laravel-mongodb": "~4.2.0",
        "morpher/ws3-client": "^1.0",
        "orchestra/parser": "^8.0",
        "paquettg/php-html-parser": "^2.2.1",
        "predis/predis": "^2.0",
        "ralouphie/mimey": "^1.0.2",
        "spatie/laravel-data": "^4.5.1",
        "tucker-eric/eloquentfilter": "^3.3.0",
        "vladimir-yuldashev/laravel-queue-rabbitmq": "^13.0",
        "wamania/php-stemmer": "^3.0"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.7.0",
        "barryvdh/laravel-ide-helper": "^3.0.0",
        "fakerphp/faker": "^1.19.0",
        "laravel/dusk": "^8.1.1",
        "laravel/sail": "^1.15.0",
        "laravel/telescope": "^5.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.10.0",
        "phpunit/phpunit": "^10.0",
        "spatie/laravel-ignition": "^2.5.1"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
